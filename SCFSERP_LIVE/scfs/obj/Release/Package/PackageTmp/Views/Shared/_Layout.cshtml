    <script>

        // jQuery plugin to prevent double submission of forms
        saving_record = 0; //set variable to show if form is in submission
        $(document).ready(function () {
            $("#submitval").val("0");            
            $('form').on('submit', function (e) {
                var saving_record = $("#submitval").val();
                if (saving_record == "0") { //If not submitted yet, submit form
                    saving_record = "1";
                    $("#submitval").val(saving_record); //Set variable to show form is in submission
                    $(this).find("input[type='submit']").prop('disabled', true);
                    $.ajax({
                        success: function (data) {
                            saving_record = "0"; //Reset variable to allow record update
                            $("#submitval").val(saving_record);
                            $(this).find("input[type='submit']").prop('disabled', false);
                        },
                        error: function (data) {
                            saving_record = "0"; //Reset variable to allow record update in case of error
                            $("#submitval").val(saving_record);
                            $(this).find("input[type='submit']").prop('disabled', false);
                        }
                    })
                }
                //else {
                //    $(this).find("input[type='submit']").prop('disabled', true);
                //    e.preventDefault();
                //}
                saving_record = "0"; //Reset variable to allow record update in case of error
                $("#submitval").val(saving_record);
                $(this).find("input[type='submit']").prop('disabled', false);
            });
        });


        function isValidDate(txtDate) {
            //return txtDate instanceof Date && !isNaN(date.getTime());
            var currVal = txtDate;            
            var dtVal = txtDate.split('/');
            if (dtVal == null)
                dtVal = txtDate.split('-');
            //Declare Regex  

            //var rxDatePattern = /^[0-9,1-9][/ | -][0-9,1-9][/ | -][0000-9999]*$/; ///^(d{1,2})(/ | -)(d{ 1, 2 }) (/|-)(d{4})$/;
            var rxDatePattern = /^(?:(?:31(\/|-|\.)(?:0?[13578]|1[02]))\1|(?:(?:29|30)(\/|-|\.)(?:0?[13-9]|1[0-2])\2))(?:(?:1[6-9]|[2-9]\d)?\d{2})$|^(?:29(\/|-|\.)0?2\3(?:(?:(?:1[6-9]|[2-9]\d)?(?:0[48]|[2468][048]|[13579][26])|(?:(?:16|[2468][048]|[3579][26])00))))$|^(?:0?[1-9]|1\d|2[0-8])(\/|-|\.)(?:(?:0?[1-9])|(?:1[0-2]))\4(?:(?:1[6-9]|[2-9]\d)?\d{2})$/
            var dtArray = currVal.match(rxDatePattern); // is format OK?
            //alert(dtVal);
            //alert(dtArray);
            //Checks for mm/dd/yyyy format.
            dtMonth = eval(dtVal[1]);
            dtDay = eval(dtVal[0]);
            dtYear = eval(dtVal[2]);
            //alert(dtMonth);
            if (currVal == '' || dtArray == null)
                return false;
            else if (dtMonth < 1 || dtMonth > 12)
                return false;
            else if (dtDay < 1 || dtDay > 31)
                return false;
            else if ((dtMonth == 4 || dtMonth == 6 || dtMonth == 9 || dtMonth == 11) && dtDay == 31)
                return false;
            else if (dtMonth == 2) {
                var isleap = (dtYear % 4 == 0 && (dtYear % 100 != 0 || dtYear % 400 == 0));
                if (dtDay > 29 || (dtDay == 29 && !isleap))
                    return false;
            }
            else
                return true;
        }
