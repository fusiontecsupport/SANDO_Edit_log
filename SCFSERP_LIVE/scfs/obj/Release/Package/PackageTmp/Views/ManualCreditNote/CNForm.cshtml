                                        var returnurl = '@Url.Action("Index", "ManualCreditNote")';
                                        window.location.href = returnurl;
                                    }
                                }
                            });

                        }
                    },
                    error: function (ex) {
                        alert('Failed to save data');
                    }
                });
            }
        });



    </script>
