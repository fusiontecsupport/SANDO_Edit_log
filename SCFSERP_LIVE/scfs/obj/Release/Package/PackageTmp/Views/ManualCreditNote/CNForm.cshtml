                    data: tab,
                    success: function (data) {
                        if (data == "Error") {
                            @*var returnurl = '@Url.Action("Index", "ESealGateIn")';
                            window.location.href = returnurl;*@
                            $('<div></div>').dialog({
                                modal: true,
                                title: "Alert",
                                open: function () {
                                    var markup = '<b>Error in Manual Credit Note Details Inserting ...!!<b>';
                                    $(this).delay(1200).fadeOut(1000);
                                    $(this).html(markup);
                                },
                                buttons: {
                                    Ok: function () {
                                        $(this).dialog("close");
                                        var returnurl = '@Url.Action("Index", "Manual CreditNote")';
                                        window.location.href = returnurl;
                                    }
                                }
                            });

                        }
                        else if (data == "Success") {
                            @*var returnurl = '@Url.Action("Index", "ESealGateIn")';
                            window.location.href = returnurl;*@
                            $('<div></div>').dialog({
                                modal: true,
                                title: "Information",
