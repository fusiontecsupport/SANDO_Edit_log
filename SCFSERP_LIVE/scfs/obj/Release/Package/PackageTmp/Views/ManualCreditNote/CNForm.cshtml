                                                    <div class="form-group">
                                                        <label>Amount<span class="required-field" /></label>
                                                        @Html.TextBoxFor(model => Model.masterdata[0].TRANREFAMT, new { @class = "form-control  TRANREFAMT", @id = "TRANREFAMT", tabindex = "5003" })
