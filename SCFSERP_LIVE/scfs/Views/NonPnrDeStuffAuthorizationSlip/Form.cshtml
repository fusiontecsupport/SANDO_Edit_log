                                    {
                                        @Html.DropDownListFor(model => model.detaildata[0].LCATEID, new SelectList(ViewBag.LCATEID, "value", "text", @Model.detaildata[0].LCATEID), new { @class = "form-control  LCATEID", @id = "LCATEID", tabindex = "3" })
                                    }
                                    else
                                    { @Html.DropDownListFor(model => model.detaildata[0].LCATEID, new SelectList(ViewBag.LCATEID, "value", "text"), new { @class = "form-control  LCATEID", @id = "LCATEID", tabindex = "3" })}

                                </div>
                            </div>

                            <div class="col-md-2">

                                <div class="form-group">
                                    <label>DeStuff Type </label>
