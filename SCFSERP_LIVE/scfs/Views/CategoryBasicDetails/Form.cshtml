@section Scripts {
    <script type="text/javascript" src="~/Scripts/CommonValidation.js"></script>
    @Scripts.Render("~/bundles/jqueryval")
<script>
        $(".CATENAME").keyup(function () {
            $(".CATEDNAME").val($(".CATENAME").val());
            $(".CATECODE").val($(".CATENAME").val().substring(0,3));
        });

        $(document).ready(function () {
            var grpname = '@Session["Group"]';
            // alert(grpname);
            if (grpname == "SuperAdmin" || grpname == "Admin") {
                document.getElementById("gstid").style.display = "block";
            }
            else {
                document.getElementById("gstid").style.display = "none";
            }

            $('#HSNID').val(1);
            $('#TARIFFMID').val(2);
        });

    $('#btnsave').click(function () {

        flag = true;

        if ($('#CATETID').val() == "") {
            alert('Please Select Category Type !!');
            $('#CATETID').focus();
            flag = false;
            //return false;
        }
        else if ($('#CATENAME').val() == "") {
            alert('Please Enter Name !!');
            $('#CATENAME').focus();
            flag = false;
            //return false;
        }
        else if ($('#CATECODE').val() == "") {
            alert('Please Enter Code !!');
            $('#CATECODE').focus();
            flag = false;
            //return false;
        }

        var pdate = new Date();

        if ($('#CATEID').val() == "") { $('#CATEID').val(0); }

        if ($('#TARIFFMID').val() == "") { $('#TARIFFMID').val(2); }

        if ($('#ACHEADID').val() == "") { $('#ACHEADID').val(143); }

        if ($('#STATEID').val() == "") { $('#STATEID').val(1); }

        if ($('#HSNID').val() == "") { $('#HSNID').val(1); }

        if ($('#CATETDSEXPRN').val() == "") { $('#CATETDSEXPRN').val('0.00'); }

        if ($('#CATEBTYPE').val() == "") { $('#CATEBTYPE').val(0); }

        if ($('#CATEPHNID').val() == "") { $('#CATEPHNID').val('0000'); }

        if ($('#CATEFNOD').val() == "") { $('#CATEFNOD').val('0.00'); }

        if ($('#CATESTRCALCTYPE').val() == "") { $('#CATESTRCALCTYPE').val('0'); }

        if ($('#CATEADDR').val() == "") { $('#CATEADDR').val('-'); }

        var tab = {
            CATEID: $('#CATEID').val(),
