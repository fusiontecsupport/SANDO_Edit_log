                    <i class="fa fa-search"></i>
                    <strong>Seal Detail</strong>
                </div>
            </div>
            <div class="box-content" data-ng-app="myModule">
                <form action="@Url.Content("~/Seal/savedata")" method="post" accept-charset="utf-8" id="Sealform" data-ng-controller="myController" name="Stuffingform" onsubmit="return Validate()">
                    @Html.ValidationSummary(true)
                    <fieldset id="ExportCartingOrder_info">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="col-md-2 hidden">
