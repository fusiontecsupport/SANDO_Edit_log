                    else if ($('.TDETAIL_BODY tr .GIDID')[i].value == "" || $('.TDETAIL_BODY tr .GIDID')[i].value == "0") {
                        alert("Please select Container No !!");
                        $('.TDETAIL_BODY tr .GIDID')[i].focus();
                        flag = false;                      
                    }
                    else if ($('.TDETAIL_BODY tr .STFDNOP')[i].value == "") {
                        alert("Please enter Nop !!");
                        $('.TDETAIL_BODY tr .STFDNOP')[i].focus();
                        flag = false;                     
                    }
                    else if ($('.TDETAIL_BODY tr .STFDQTY')[i].value == "") {
                        alert("Please enter Weight !!");
                        $('.TDETAIL_BODY tr .STFDQTY')[i].focus();
                        flag = false;                     
                    }
                    else if ($('.TDETAIL_BODY tr .STFDSBDDATE')[i].value != "" && $(".STFMDATE").val() != "") {
                        var codate = Splitthedate($('.TDETAIL_BODY tr .STFDSBDDATE')[i].value);
                        //alert(codate);
                        var sealdate = Splitthedate($(".STFMDATE").val());
                        //alert(sealdate);
                        if (sealdate < codate) {

                            alert("Seal date must be grater than C.Order date");
                            $('#STFMDATE').focus();
                            flag = false;                         
                        }
                    }
                    i++;    
                });
            }
            if (flag) {
                var r = confirm("Are you sure to submit this record ?");
                if (r == true) {
                    return true;
                } else {

                    return false;

                }
            } else {

                return false;
            }

        }


