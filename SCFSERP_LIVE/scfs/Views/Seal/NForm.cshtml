        function GetFormattedDate(obj) {
            var MyDate_String_Value = obj;
            var value1 = new Date
                (
                    parseInt(MyDate_String_Value.replace(/(^.*\()|([+-].*$)/g, ''))
                );
            var dat = value1.getDate() +
                "-" +
                eval(value1.getMonth() + 1) +
                "-" +
                value1.getFullYear();
            return dat;
        }
