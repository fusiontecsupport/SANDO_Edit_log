                flag = false;               
            }
            else if ($(".STFBCHAGSTNO").val() == "" || $(".STFBCHAADDR1").val() == "") {
                alert("Please check Billing Address / GST properly !! ");
                $("#STFBCHAGSTNO").focus();
                flag = false;                
            }
            else if ($(".EOPTID").val() == "") {
                alert("Please Select Operation Type !! ");
                $(".EOPTID").focus();
                flag = false;               
            }
            if (flag) {
                $('#TDETAIL_BODY tr').each(function () {
                    if ($('.TDETAIL_BODY tr .STFCORDID')[i].value == "" || $('.TDETAIL_BODY tr .STFCORDID')[i].value == "0") {
