@using scfs.Data;
@using scfs_erp.Models;

@{
    ViewBag.Title = "Non PNR Vehicle Ticket";
}

<div class="col-lg-12">
    <div class="panel panel-primary" style="background: #323C55;">
        <div class="panel-heading">
            <div class="panel-title">
                <i class="fa fa-bar-chart-o fa-fw"></i>Non-PNR Vehicle Ticket
                <a class="btn btn-success btn-sm pull-right" style="padding:3px;margin:0 0 10px" href="@Url.Action("NForm", "NonPnrVehicleTicket")"><i class="fa fa-plus-square "></i> Add New </a>

            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="panel-body">
                <div class="row">
                    <div class="col-xs-6">
                        <form action="#" method="post" accept-charset="utf-8" id="exportgateinform" data-ng-controller="myController" name="exportgateinform">
                            <div class="col-md-12">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Type</label>
                                        @Html.DropDownList("TRANBTYPE", null, new { @class = "form-control  TRANBTYPE", @id = "TRANBTYPE", tabindex = "2" })
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label for="from">From</label>
                                        <input type="text" id="from" class="form-control" name="from" value="@System.Web.HttpContext.Current.Session["SDATE"]">
                                    </div>
                                </div>

                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label for="to">To</label>
                                        <input type="text" id="to" class="form-control" name="to" value="@System.Web.HttpContext.Current.Session["EDATE"]">
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label for="to"></label>
                                        <input type="submit" name="submit" value="Search" class="form-control btn btn-primary btn-xs" />
                                    </div>
                                </div>
                            </div>

                            @*<input type="button" name="submit" value="Search" id="btnsearch" class="btn btn-primary btn-xs" />*@
                        </form>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <table class="table table-bordered table-striped table-hover table-heading table-datatable" id="dataTables-example">
                            <thead>
                                <tr>
                                    <th>
                                        Date
                                    </th>
                                    <th>
                                        No
                                    </th>

                                    <th>
                                        Container No
                                    </th>
                                    <th>
                                        IGM No
                                    </th>
                                    <th>
                                        Line No
                                    </th>
                                    <th>
                                        Size
                                    </th>
                                    <th>
                                        A.Slip No
                                    </th>
                                    <th> A.Slip Type</th>
                                    <th>CHA Name</th>
                                    <th>BOE No</th>
                                    <th class="hide"></th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tfoot>
                                <tr>
                                    <th colspan="10" style="text-align:center"></th>
                                </tr>

                            </tfoot>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts {

    <script>
        $(document).ready(function () {

            $('.btncreate').click(function () {


            });

            $('#dataTables-example').dataTable({
                "bServerSide": true,
                "sAjaxSource": "@Url.Action("GetAjaxData", "NonPnrVehicleTicket")",
                "bProcessing": true,
                "bStateSave": true,
                //"aaSorting": [[1, "asc"]],
                "oLanguage": {
                    "sSearch": "Search: "
                },
                "aoColumns": [
                    { "sName": "", "sClass": "col-lg-1" },
                    { "sName": "", "sClass": "col-lg-1" },
                    { "sName": "", "sClass": "col-lg-1" },
                    { "sName": "", "sClass": "col-lg-1" },
                    { "sName": "", "sClass": "col-lg-1" },
                    { "sName": "", "sClass": "col-lg-1" },
                    { "sName": "", "sClass": "col-lg-1" },
                    { "sName": "", "sClass": "col-lg-1" },
                    { "sName": "", "sClass": "col-lg-2" },
                    { "sName": "", "sClass": "col-lg-1" },
                    { "sName": "", "sClass": "col-lg-1 hide" },
                    {
                        "sClass": "col-sm-1 cent",
                        "sName": "ID",
                        "bSearchable": false,
                        "bSortable": false,
                        "fnRender": function (oObj) {
                             var edchk = '';
                            var dlchk = '';
                            if (oObj.aData[14] == 'Not-Exists') {
                                edchk = ' <a   class="btn btn-primary btn-xs"   href="@Url.Action("Edit", "NonPnrVehicleTicket")/' +
                                oObj.aData[10] + '\"><i class="glyphicon glyphicon-edit"></i></a>  ';
                                dlchk = '<a  class="btn btn-danger del btn-xs" href="#" id="' + oObj.aData[10] + "-" + oObj.aData[11] + "-" + oObj.aData[12] + "-" + oObj.aData[13] + '" "><i class="glyphicon glyphicon-trash"></i> </a> ';

                            }

                            return edchk+'&nbsp;&nbsp;<a   class="btn btn-default btn-xs"  target="_blank" href="@Url.Action("PrintView", "NonPnrVehicleTicket")/' +
                                oObj.aData[10] + '\"><i class="glyphicon glyphicon-print" ></i></a>  &nbsp;&nbsp;<a   class="btn btn-success btn-xs"  target="_blank"  title="QR Code Print" href="@Url.Action("QRCPrintView", "NonPnrVehicleTicket")/' +
                                oObj.aData[10] + '\"><i class="glyphicon glyphicon-print" ></i></a>    &nbsp;&nbsp;' + dlchk;
                        }
                    }

                ],
                "fnFooterCallback": function (nRow, aaData, iStart, iEnd, aiDisplay) {
                    var TNOS = 0; var FNOS = 0; var FFNOS = 0; var TUES = 0;

                    /*
                     * Calculate the total market share for all browsers in this table (ie inc. outside
                     * the pagination)
                     */

                    //alert(aiDisplay.length + "//" + nRow);
                    for (var i = 0; i < aaData.length; i++) {
                        // alert(aaData[4]);

                    }



                    /* Calculate the market share for browsers on this page */

                    for (var i = 0; i < aaData.length; i++) {
                        //alert("2" + aaData[aiDisplay[i]][3]);
                        if (aaData[aiDisplay[i]][5] == 20) TNOS++;
                        if (aaData[aiDisplay[i]][5] == 40) FNOS++;
                        if (aaData[aiDisplay[i]][5] == 45) FFNOS++;
                        TUES = eval(TNOS) + eval(2 * FNOS) + eval(2 * FFNOS);
                        //  iPageMarket += aaData[aiDisplay[i]][4] * 1;
                    }

                    /* Modify the footer row to match what we want */
                    var nCells = nRow.getElementsByTagName('th'); nCells[0].innerHTML = '<span style="color:red">20":</span>' + eval(TNOS) + '&nbsp;&nbsp;&nbsp;&nbsp;<th></th>' + '<span style="color:red">40":</span>' + eval(FNOS) + '&nbsp;&nbsp;&nbsp;&nbsp;<th></th>' + '<span style="color:red">45":</span>' + eval(FFNOS) + '&nbsp;&nbsp;&nbsp;&nbsp;<th></th>' + '<span style="color:red">TUES:</span>' + eval(TUES)


                }

            });

            $("#from").datepicker({
                defaultDate: "+1w",
                changeMonth: true,
                numberOfMonths: 3,
                dateFormat: 'dd-mm-yy',
                onClose: function (selectedDate) {
                    $("#to").datepicker("option", "minDate", selectedDate);
                }
            });
            $("#to").datepicker({
                defaultDate: "+1w",
                changeMonth: true,
                numberOfMonths: 3,
                dateFormat: 'dd-mm-yy',
                onClose: function (selectedDate) {
                    $("#from").datepicker("option", "maxDate", selectedDate);
                }
            });
            //------------Delete Record-------------//
            $(document).on("click", "a.del", function () {
                var r = confirm("Are you sure to delete this record ?");

                if (r == true) {
                    var idx = $(this).attr('id');
                    base = window.location.origin;
                    $.post(base + "/NonPnrVehicleTicket/Del",
                        {
                            id: idx, fld: "GATEOUT"
                        },
                        function (data, status) {
                            if (data == "") {
                                //alert("You are not Authorized to Delete....!");

                                $('<div></div>').dialog({
                                    modal: true,
                                    title: "Information",
                                    open: function () {
                                        var markup = '<b>' + 'You are not Authorized to Delete....!' + '<b>';
                                        $(this).html(markup);
                                    },
                                    buttons: {
                                        Ok: function () {
                                            $(this).dialog("close");
                                        }
                                    }
                                }); //end confirm dialog
                            }
                            else if (data != "Deleted successfully...") {
                               // alert(data);

                                $('<div></div>').dialog({
                                    modal: true,
                                    title: "Information",
                                    open: function () {
                                        var markup = '<b>' + data + '<b>';
                                        $(this).html(markup);
                                    },
                                    buttons: {
                                        Ok: function () {
                                            $(this).dialog("close");
                                        }
                                    }
                                }); //end confirm dialog
                            }
                            else {
                                //alert(data);

                                $('<div></div>').dialog({
                                    modal: true,
                                    title: "Information",
                                    open: function () {
                                        var markup = '<b>' + data + '<b>';
                                        $(this).html(markup);
                                    },
                                    buttons: {
                                        Ok: function () {
                                            $(this).dialog("close");
                                        }
                                    }
                                }); //end confirm dialog
                                location.replace(base + "/NonPnrVehicleTicket");
                            }
                        });
                }
                return false;
            });//End
        });
    </script>

