                        //$("#STMRNAME").val(data[0].STMRNAME);
                        //$("#OSMCNAME").val(data[0].CFSNAME);
                        //$("#FVTDNO").val(data[0].VTDNO);

                        $("#VHLNO").val(data[0].VHLNO);
                        $("#GDRVNAME").val(data[0].DRVNAME);

                        //if (data[0].ASLMTYPE == 1) {
