                        $("#GOTRNSPRTNAME").val(data[0].GOTRNSPRTNAME);
                        $("#CHASNAME").val(data[0].CHANAME);
                        $("#CHANAME").val(data[0].CHANAME);



                        $("#CHAID").val(data[0].CHAID);
