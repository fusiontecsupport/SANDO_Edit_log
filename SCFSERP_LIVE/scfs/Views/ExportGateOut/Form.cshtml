            //    alert("Please Enter SANCO Seal No. !!");
            //    $("#LSEALNO").focus();
            //    //return false;
            //    flag = false;
            //}
            if (flag) {
                $('#GODATE').css('border-color', 'black');
                $('#GOTIME').css('border-color', 'black');
                $('#FVTDNO').css('border-color', 'black');

                if (($('#GODATE').val() != "" && $('#GODATE').val() != null) && ($('#MVTMDATE').val() != "" && $('#MVTMDATE').val() != null)) {
                    var dsdate = Splitthedate($('#GODATE').val());
                    var osdate = Splitthedate($('#MVTMDATE').val());
                   
                    if ((dsdate < osdate)) {
                        alert("Gate Out Date Grater Than VT Date !! ");
                        $("#GODATE").focus();
                        flag = false;
                    }
                }
                if (flag) {
                    var r = confirm("Are you sure you want to submit this record ?");
                    if (r == true) {
                        return true;
                    } else {
                        return false;
                    }
                }
                else {
                    return false;
                }
            }
        }

